                 android:name=".ChangeListSizeActivity" >
                 
             </activity>
             <activity
                 android:name=".DisplayAppVersionActivity" >
                 
             </activity>
             <activity 
                 android:name=".SortingActivity"></activity>
    </application>

</manifest>
